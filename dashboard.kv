<On_active_button@Button_Item>
    icon_color: app.theme_cls.text_color
    text_color: app.theme_cls.text_color
    button_bg_color: 23/255,28/255,35/255,1
    mode: "color_on_active"
    badge_disabled: True

<MDIconButtonOff@MDIconButton>
    user_font_size: "40sp"
    pos_hint:{"center_y": .5}
    theme_text_color: "Custom"
    # text_color: 180/255,180/255,180/255,1 # light color
    text_color: 23/255,28/255,35/255,1 # dark color
    # text_color: 14/255,78/255,107/255,1 # blue-accent color

<PilLayout@BoxLayout>
    size_hint: .1, .05
    padding:"7dp"
    canvas:
        Color:
            rgba:(6/255,17/255,21/255,1)
        RoundedRectangle:
            size: self.size
            pos: self.pos

<RoundCard@MDCard>
    md_bg_color:23/255,28/255,35/255,1
    spacing:"1dp"
    padding:"5dp"
    size_hint:1,0.2
    border_radius:50
    radius:[5]

<RoundCard_data@MDCard>
    orientation: "horizontal"
    spacing:"30dp"
    padding:"30dp"
    size_hint:0.7,1
    size: "300dp", "120dp"
    md_bg_color: 255/255,255/255,255/255,1
    border_radius:50
    radius:[5]

<Progress>
    MDDialog:
        title:'Tersambung'
        radius:[7, 7, 7, 7],
        md_bg_color:244/255,67/255,54/255,1
        size_hint:None, None
        size:400, 400

